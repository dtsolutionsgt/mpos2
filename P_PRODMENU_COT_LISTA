CREATE TABLE [dbo].[P_PRODMENU_COT_LISTA](
	[CODIGO_MENU_LISTA] [int] IDENTITY(1,1) NOT NULL,
	[CODIGO_MENU] [int] NULL FOREIGN KEY REFERENCES P_PRODMENU_COT(CODIGO_MENU),
	[CODIGO_PRODUCTO] [int] NOT NULL FOREIGN KEY REFERENCES P_PRODUCTO(CODIGO_PRODUCTO),
	[CANT] [float] NOT NULL,
	[NOTA] [nvarchar](50) NULL
 CONSTRAINT [PK_P_PRODMENU_COT_LISTA] PRIMARY KEY CLUSTERED 
(
	[CODIGO_MENU_LISTA] ASC
)WITH (PAD_INDEX = OFF, STATISTICS_NORECOMPUTE = OFF, IGNORE_DUP_KEY = OFF, ALLOW_ROW_LOCKS = ON, ALLOW_PAGE_LOCKS = ON) ON [PRIMARY]
) ON [PRIMARY]
