CREATE TABLE [dbo].[P_PRODMENUOPC_LIST](
	[CODIGO_MENUOPC_LIST] [int] IDENTITY(1,1) NOT NULL,
	[CODIGO_MENU_OPCION] [int] NULL FOREIGN KEY REFERENCES P_PRODMENUOPC_COT(CODIGO_MENU_OPCION),
	[CODIGO_PRODUCTO] [int] NOT NULL FOREIGN KEY REFERENCES P_PRODUCTO (CODIGO_PRODUCTO),
	[CANT] [float] NOT NULL
 CONSTRAINT [PK_CODIGO_MENU_OPC_LIST] PRIMARY KEY CLUSTERED 
(
	[CODIGO_MENUOPC_LIST] ASC
)WITH (PAD_INDEX = OFF, STATISTICS_NORECOMPUTE = OFF, IGNORE_DUP_KEY = OFF, ALLOW_ROW_LOCKS = ON, ALLOW_PAGE_LOCKS = ON) ON [PRIMARY]
) ON [PRIMARY]
